    hidden = KAppIsHidden;
    embeddability = KAppEmbeddable;  
    localisable_resource_file = EVTHND_RESOURCE_FILE_NAME;
    localisable_resource_id = R_EVTHND_LOCALISABLE_APP_INFO;
    }

